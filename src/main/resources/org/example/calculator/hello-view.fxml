<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="240.0" prefWidth="279.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="org.example.calculator.MyController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="109.0" prefWidth="268.0">
         <children>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="51.0" prefWidth="244.0">
               <children>
                  <TextField fx:id="inputLeft" layoutX="19.0" layoutY="13.0" prefHeight="25.0" prefWidth="73.0" />
                  <TextField fx:id="inputRight" layoutX="158.0" layoutY="13.0" prefHeight="25.0" prefWidth="73.0" />
               </children>
            </AnchorPane>
          <AnchorPane layoutX="-10.0" layoutY="48.0" minHeight="0.0" minWidth="0.0" prefHeight="50.0" prefWidth="269.0">
               <children>
                  <Button fx:id="buttonPlus" layoutX="20.0" layoutY="14.0" mnemonicParsing="false" text="+" textAlignment="CENTER" />
                  <Button fx:id="buttonMinus" layoutX="71.0" layoutY="14.0" mnemonicParsing="false" text="-" textAlignment="CENTER" />
                  <Button fx:id="buttonStar" layoutX="112.0" layoutY="14.0" mnemonicParsing="false" text="*" textAlignment="CENTER" />
                  <Button fx:id="buttonPer" layoutX="157.0" layoutY="13.0" mnemonicParsing="false" text="/" textAlignment="CENTER" />
                  <Button fx:id="buttonSzazalek" layoutX="209.0" layoutY="14.0" mnemonicParsing="false" text="\%" textAlignment="CENTER" />
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
         <children>
            <Label fx:id="outputLabel" layoutX="110.0" layoutY="33.0" text="Label" />
         </children>
      </AnchorPane>
   </children>
</VBox>
